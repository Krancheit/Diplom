	{
  	TFormSpectrTrend* prSigForm;
  	trSpectrTrendCallback() { prSigForm = NULL; }
  	virtual void SetPosition(double fPos)
  	{ }
  	virtual long GetRightBorder()
  	{ return -1; }
  	virtual TrMcExamine* GetExamine()
  	{ return NULL; }
